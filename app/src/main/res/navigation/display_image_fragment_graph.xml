<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/display_image_fragment_graph"
    app:startDestination="@id/blankFragment">

    <action
        android:id="@+id/show_artworkDetail"
        app:destination="@id/blankFragment" />

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.listocalixto.android.rembrandt.feature.displayimage.BlankFragment"
        android:label="BlankFragment"
        tools:layout="@layout/fragment_blank">
        <argument
            android:name="artworkId"
            app:argType="integer"
            app:nullable="false" />
        <action
            android:id="@+id/action_blankFragment_self"
            app:destination="@id/blankFragment" />
    </fragment>

</navigation>
